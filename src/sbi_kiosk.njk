	{# {% include "section_product_page_hero.njk" %} #}
	<section class="block product_page_hero layout vertical horizontal-md center" type="product_page_hero">
	<div class="descbox layout inline vertical flex wider">
		<div class="subtitle layout inline horizontal end">
			{% if product.sub_header %}
				<div>{{ product.sub_header }}</div>
			{% endif %}
			{% if product.icon %}
				<img src="{{ product.icon }}" alt="Eloka Logo" style="height:50px">
			{% endif %}
		</div>

		<p class="sub_tag_line">{{ product.tag_line }}</p>

		<h1>{{ product.sub_tag_line }}</h1>

