{% extends "./base.njk" %}

{% block main %}
<main>

    <div class="flex-grid" id="links">
        {% for item in ekoUniversity %}
            <div>
                <p><a href="/eko-university/{{ item.title | slug }}">{{ item.title }}</a></p>
            </div>
        {% endfor %}
    </div>

    <div class="pure-g">
    {% for id in ekoUni.ids %}
        <div class="pure-u-1-2 pure-u-md-1-2 pure-u-sm-1 l-box">
        <p>{% youtube id %}</p>
        </div>
    {% endfor %}
    </div>
    {% sectionSeparator %}
</main>
{% endblock %}