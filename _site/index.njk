---
title: "Home"
layout: base
---
<main class="container">
<br><br><br><br>

{# Top Hero #}
<center>
    <p>{{ main.top_hero.tag }} - <a href='{{ main.top_hero.tag_link }}'>{{ main.top_hero.tag_link_text }}</a></p>
    <h1>{{ main.top_hero.header }}</h1>
    <br>
    <a href=" {{ main.top_hero.button_link }}" class="btn btn-secondary">{{ main.top_hero.button }}</a>
    <br><br>
    <img src="{{ main.top_hero.image }}" alt="top hero">
</center>

{% sectionSeparator %}

{# Mid Summary #}
<div class="row">
    <div class="col-8">
        <h1>{{ main.mid_summary.header }}</h1>
    </div>
</div>
<br>
<div class="row">
    <div class="col-10">
        <p>{{ main.mid_summary.description }}</p>
    </div>
</div>

{% sectionSeparator %}

{# Slider #}
<h1>insert slider here</h1>

{% sectionSeparator %}

{# Short Ad #}
<div class="row">
    <div class="col">
        <br>
        <img src="{{ main.short_ad.image }}">
    </div>
    <div class="col">
        <h1>{{ main.short_ad.header }}</h1>
        <br>
        <p>{{ main.short_ad.text }}</p>
        <a href="{{ main.short_ad.link }}">{{ main.short_ad.link_text }}</a>
        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-right" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"/>
        </svg>
    </div>
</div>

{% sectionSeparator %}

{# Icon List #}
<center>
    <h1>{{main.icon_list[0].header}}</h1>
    <br>
</center>
<div class="row">
    {% for i in range(1, main.icon_list | length) %}
        {% set item=main.icon_list[i] %}
        <div class="col">
        <center>
            <img src="{{ item.image }}">
            <h3>{{ item.text }}</h3>
        </center>
        </div>
    {% endfor %}
</div>

{% sectionSeparator %}

{# Side-by-Side Heros #}
<div class="row">
    <div class="col">
        <center>
            <img src="{{ main.side_by_side.image_left }}">
            <h2>{{ main.side_by_side.title_left }}</h2>
            <p>{{ main.side_by_side.text_left }}</p>
        </center>
    </div>
    <div class="col-md-2 col-md-offset-2"></div>
    <div class="col">
        <center>
            <img src="{{ main.side_by_side.image_right }}"> 
            <h2>{{ main.side_by_side.title_right }}</h2>
            <p>{{ main.side_by_side.text_right }}</p>
        </center>
    </div>
</div>

{% sectionSeparator %}

{# One Liner #}
<center>
    <img src="{{ main.one_liner.icon }}" alt="{{ main.one_liner.title }}">
    <h1>{{ main.one_liner.title }}</h1>
    <p>{{ main.one_liner.text }}</p>
</center>

{% sectionSeparator %}

{# Alternating Steps #}
<center>
    <h6>{{ main.alternating_steps[0].small_header }}</h6>
    <h1>{{ main.alternating_steps[1].main_header }}</h1>
    <br><br>
</center>

{% set align = "left" %}
{% for i in range(2, main.alternating_steps | length) %}
    {% set item=main.alternating_steps[i] %}
    <div class="container">
    {% if align == "left" %}
        <div class="row">
            <div class="col">
                <br>
                <h3>{{ item.title }}</h3>
                <p>{{ item.text }}</p>
                <p>{{ item.tag }}</p>
                <div class="row">
                <div class="col">
                <a href="{{ item.button_link }}" class="btn btn-secondary">{{ item.button_text }}</a>
                </div>
                <br><br>
                <div class="col">
                <a href="{{ item.link }}">{{ item.link_text }}</a>
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-right" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"/>
                </svg>
                </div>
            </div>
            </div>
            <div class="col">
                <img src="{{ item.image }}">
            </div>
        </div>
        {% set align = "right" %}
    {% elif align == "right" %}
    <div class="row">
        <div class="col">
            <img src="{{ item.image }}">
        </div>
        <div class="col">
            <br>
            <h3>{{ item.title }}</h3>
            <p>{{ item.text }}</p>
            <p>{{ item.tag }}</p>
            <div class="row">
                <div class="col">
                <a href="{{ item.button_link }}" class="btn btn-secondary">{{ item.button_text }}</a>
                </div>
                <br><br>
                <div class="col">
                <a href="{{ item.link }}">{{ item.link_text }}</a>
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-right" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z"/>
                </svg>
                </div>
            </div>
        </div>
    </div>
    {% set align = "left" %}
    {% endif %} 
    </div> 
{% endfor %}

{% sectionSeparator %}

{# Header #}
<div class="row">
    <div class="col-2"></div>
    {% for i in range(1, main.header | length) %}
        {% set tag=main.header[i].text %}
            <div class="col-2">
                <center><p>{{ tag }}</p><center>
            </div>
    {% endfor %}
    <div class="col-2"></div>
</div>
<center><h1>{{ main.header[0].title }}</h1></center>

{% sectionSeparator %}

{# Gallery #}
<h1>{{ main.gallery[0].header}}</h1>
<h6>insert gallery slideshow</h6>
{# Finish Gallery #}

{% sectionSeparator %}

{# Small Heros Section #}
<center>
    <h1>{{ main.small_heros_section[0].header}}
</center>
<p><br><br><br></p>
<div class="row">
    {% for i in range (1, main.small_heros_section | length) %}
        {% set item=main.small_heros_section[i]%}
        <div class="col">
            <img src="{{ item.image }}">
        </div>
    {% endfor %}
</div>

{% sectionSeparator %}

{# Form #}
<center>
    <h1>{{ main.form.header }}</h1>
    <div class="row"><br></div>
    <p>{{ main.form.explanation }}</p>
    <form action="">
</center>

<form action="">
    <center>
        <input type="text" size="75" id="entry1" name="{{ main.form.entry_boxes[0] }}" value="{{ main.form.entry_boxes[0] }}"><br>
    </center>
    <div class="row">
        <div class="col"></div>
        <div class="col">
            <br><input type="text" size="35" id="entry2" name="{{ main.form.entry_boxes[1] }}" value="{{ main.form.entry_boxes[1] }}"><br>
        </div>
        <div class="col">
            <br><input type="text" size="35" id="entry3" name="{{ main.form.entry_boxes[2] }}" value="{{ main.form.entry_boxes[2] }}"><br>
        </div>
        <div class="col"></div>
    </div>
    <center>
        <br><input type="text" size="75" height="400px" id="entry4" name="{{ main.form.entry_boxes[3] }}" value="{{ main.form.entry_boxes[3] }}"><br>
        <br><br><input type="submit" value="Submit">
    </center>
</form>

{% sectionSeparator %}

{# Big Ad #}
<div class="row">
<div class="col">
    <h3>{{ main.big_ad.small_header }}</h3>
    <h1>{{ main.big_ad.big_header }}</h1>
    <br>
    <div class="row">
        <div class="col-3">
        <center>
            <img src="{{ main.big_ad.icon1 }}">
            <p>{{ main.big_ad.caption1 }}</p>
        </center>
        </div>
        <div class="col-3">
        <center>
            <img src="{{ main.big_ad.icon2 }}">
            <p>{{ main.big_ad.caption2 }}</p>
        </center>
        </div>
    </div>
</div>
<div class="col">
    <img src="{{ main.big_ad.image }}">
</div>
</main>